        "type": "apiKey",
        "in": "header",
        "name": "token"
      }
    },
    "schemas": {
      "UserType": {
        "title": "UserType",
        "required": [
          "name"
        ],
        "properties": {
          "name": {
            "type": "string"
          },
          "status": {
            "type": "string",
            "enum": [
              "enabled",
              "disabled"
            ]
          },
          "_id": {
            "type": "string"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          },
          "updatedAt": {
            "type": "string",
            "format": "date-time"
          }
        }
      },
      "User": {
        "title": "User",
        "required": [
          "email",
          "password",
          "first_name",
          "last_name",
          "identification_number",
          "birth_date"
        ],
        "properties": {
          "email": {
            "type": "string"
          },
