          },
          "first_name": {
            "type": "string"
          },
          "last_name": {
            "type": "string"
          },
          "identification_number": {
            "type": "string"
          },
          "birth_date": {
            "type": "string",
            "format": "date-time"
          },
          "user_type": {
            "type": "string"
          },
          "member_status": {
            "type": "string",
            "enum": [
              "Pendiente",
              "Verificado"
            ]
          },
          "_id": {
            "type": "string"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          },
          "updatedAt": {
            "type": "string",
            "format": "date-time"
          }
        }
      }
    }
  }
}