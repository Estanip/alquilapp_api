{
  "name": "alquilapp_api",
  "version": "0.0.1",
  "description": "NestJS API - Alquilapp",
  "author": "Estani Pettigrew",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "dev": "nest start --watch",
